GOOSE_PROVIDER=ollama
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=qwen2.5
GEMINI_API_KEY=

WP_BASE_URL=http://localhost:8080
WP_JWT_TOKEN=
WP_USERNAME=
WP_APP_PASSWORD=

ASSEMBLYAI_API_KEY=
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=

LOG_LEVEL=INFO

# Controller security (optional)
# Set a token to require X-Agent-Token on controller and mobile API commands
AGENT_TOKEN=
# Bind hosts (defaults are localhost-only)
AGENT_HOST=127.0.0.1
MOBILE_HOST=127.0.0.1
# Simple rate limit for /api/command: max/window_seconds (e.g., 30/10)
RATE_LIMIT=30/10
# Mobile CORS allowed origins (comma-separated). Leave empty to allow all in dev.
MOBILE_CORS_ORIGINS=
# Optional request signing (HMAC SHA256 of `${timestamp}.${body}`)
AGENT_SIGNING_KEY=
SIGNING_SKEW_SECONDS=300
MOBILE_RATE_LIMIT=30/10
